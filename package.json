{
  "name": "tymly-doc-generator",
  "version": "1.0.0",
  "description": "A package to help us generate tymly-docs as Markdown files",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha && standard",
    "generate": "node ./scripts/generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmfs/tymly-doc-generator.git"
  },
  "keywords": [
    "tymly",
    "package",
    "docs"
  ],
  "author": "Reece Brend",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wmfs/tymly-doc-generator/issues"
  },
  "homepage": "https://github.com/wmfs/tymly-doc-generator#readme",
  "dependencies": {
    "@wmfs/cardscript-assets": "0.0.0-semantically-released",
    "@wmfs/json-schema-markdown-a-tron": "0.0.0-semantically-released",
    "@wmfs/tymly-gatherer": "1.10.0",
    "lodash": "^4.17.11",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^5.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "chai": "4.2.0",
    "debug": "4.1.1",
    "conventional-changelog-metahub": "3.0.0",
    "cz-conventional-changelog": "3.0.2",
    "standard": "14.3.1",
    "mocha": "6.2.2",
    "nyc": "15.0.0",
    "semantic-release": "15.14.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
